concentration=
	eddy diff coef/thickness
	~	TonC/meter
	~		|

total carbon emissions=
	114
	~	KTon
	~		|

preindustrial co2=
	5.9e+11
	~	TonC
	~		|

biomass res time=
	10.6
	~	Year
	~		|

biostim coeff=
	0.4
	~	Dmnl
	~		|

buff co2 coeff=
	4.05
	~	Dmnl
	~		|

buffer factor=
	ref buffer factor+buff co2 coeff * LN( co2 in atmosphere/ref buff co2)
	~	
	~		|

co2 in atmosphere= INTEG (
	flux atm to biomass+flux atm to ocean+flux biomass to atmosphere,
		6.576e+11)
	~	TonC
	~		|

co2 in biomass= INTEG (
	-flux atm to biomass-flux biomass to atmosphere-flux biomass to humus,
		6.566e+11)
	~	TonC
	~		|

co2 in humus= INTEG (
	flux biomass to humus,
		7.259e+11)
	~	TonC
	~		|

co2 in mixed layer= INTEG (
	-diffusion flux-flux atm to ocean,
		7.712e+11)
	~	
	~		|

diffusion flux=
	(co2 in mixed layer/mixed depth-concentration)*eddy diff coef*2/(mixed depth+thickness\
		)
	~	TonC/Year
	~		|

eddy diff coef=
	4000
	~	meter*meter/Year
	~		|

equil co2 in mixed layer=
	preind co2 in mixed*(co2 in atmosphere/preindustrial co2)^(1/buffer factor)
	~	TonC
	~		|

flux atm to biomass=
	init npp*(1+biostim coeff*LN(co2 in atmosphere/preindustrial co2))
	~	TonC/Year
	~		|

flux atm to ocean=
	(equil co2 in mixed layer-co2 in mixed layer)/mixing time
	~	TonC/Year
	~		|

flux biomass to atmosphere=
	co2 in biomass/biomass res time*(1-humification fraction)
	~	TonC/Year
	~		|

flux biomass to humus=
	co2 in biomass/biomass res time*humification fraction
	~	TonC/Year
	~		|

flux humus to atmosphere=
	co2 in humus/humus res time
	~	TonC/Year
	~		|

humification fraction=
	0.428
	~	Dmnl
	~		|

humus res time=
	27.8
	~	Year
	~		|

init npp=
